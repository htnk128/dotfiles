[github]
    user = htnk128

[core]
    ignorecase = false
    editor = vim
    excludesfile = ~/.gitignore_global

[color]
    ui = auto
    diff = auto
    grep = auto
    branch = auto
    interactive = auto

[grep]
    lineNumber = true

[pull]
    rebase = false

[push]
    default = current

[commit]
    template = ~/.stCommitMsg

[ghq]
    root = ~/src
